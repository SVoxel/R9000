config GREENDOWNLOAD_GIT_URL
	string
	prompt "url of greendownload git repository"
	default "ssh://dniserver/scm/dnigit/common/apps/greendownload.git"
	help
	   Input the url of greendownload git repository which you want to build


config TRAFFICMETER_PLATFORM_PATCH
	bool "Apply platform specific patch"

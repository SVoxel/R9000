#! /bin/sh

CONFIG=/bin/config

if [ "x$($CONFIG get factory_mode)" = "x1" ]; then
	echo "factory_mode: $1" > /dev/console
	if [ "x$($CONFIG get factory_tt3)" = "x1" ]; then
		$CONFIG unset factory_tt3
		$CONFIG commit
		ledcontrol -n usb1 -c amber -s off
		echo "factory_TT3: Exit" > /dev/console
	fi
	exit 255
fi 

						};
						tx {
							override = "enabled";
							amp = <1>;
							total_driver_units = <19>;
							post_emph = <2>;
							pre_emph = <0>;
							slew_rate = <0>;
						};
					};
					lane_1_params {
						rx {
							override = "enabled";
							dcgain = <0x0>;
							dfe_3db_freq = <0x7>;
							dfe_gain = <0x0>;
							dfe_1st_tap_ctrl = <0x0>;
							dfe_2nd_tap_ctrl = <0x8>;
							dfe_3rd_tap_ctrl = <0x0>;
							dfe_4th_tap_ctrl = <0x8>;
							low_freq_agc_gain = <0x7>;

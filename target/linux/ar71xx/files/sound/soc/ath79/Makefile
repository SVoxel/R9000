snd-soc-ath79-pcm-objs := ath79-pcm.o ath79-mbox.o
snd-soc-ath79-i2s-objs := ath79-i2s.o ath79-i2s-pll.o
snd-soc-ath79-codec-objs := ath79-internal-codec.o
snd-soc-db120-objs := qca-db120.o
snd-soc-cus227-objs := qca-cus227.o

obj-$(CONFIG_SND_ATH79_SOC) += snd-soc-ath79-pcm.o
obj-$(CONFIG_SND_ATH79_SOC_I2S) += snd-soc-ath79-i2s.o
obj-$(CONFIG_SND_ATH79_SOC_CODEC) += snd-soc-ath79-codec.o
obj-$(CONFIG_SND_ATH79_SOC_DB120) += snd-soc-db120.o
obj-$(CONFIG_SND_ATH79_SOC_CUS227) += snd-soc-cus227.o

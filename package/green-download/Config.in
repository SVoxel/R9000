config GREENDOWNLOAD_GIT_TREEISH
	string
	prompt "treeish of greendownload git repository"
	default "greendownload-1.0-dni5"
	help
           Input the treeish of greendownload git repository which you want to build

config GREENDOWNLOAD_GIT_REPOSITORY
	string
	prompt "name of greendownload git repository"
	default "greendownload.git"
